<marko-core-obj-value|{ value }|
  obj=input.obj
  field=input.field
>
  <marko-core-array|{ value: v, index, length }|
    tag=input.tag
    value=value
    class=input.class
    attrs=input.attrs
  >
    <${input.renderBody} value=v index=index length=length />
  </marko-core-array>
</marko-core-obj-value>
